<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    tools:context=".Settings">


    <pl.droidsonroids.gif.GifImageView
        android:layout_width="123dp"
        android:id="@+id/musicgif"
        android:layout_height="80dp"
        android:layout_x="151dp"
        android:layout_y="113dp"
        android:src="@mipmap/ic_launcher" />

    <pl.droidsonroids.gif.GifImageView
        android:layout_width="123dp"
        android:id="@+id/profile"
        android:layout_height="75dp"
        android:layout_x="148dp"
        android:layout_y="285dp"
        android:src="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="310dp"
        android:layout_height="62dp"
        android:layout_x="108dp"
        android:layout_y="180dp"
        android:fontFamily="sans-serif-black"
        android:text="Music and Sound Settings"
        android:textSize="18sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="259dp"
        android:layout_height="62dp"
        android:layout_x="140dp"
        android:layout_y="350dp"
        android:fontFamily="sans-serif-black"
        android:text="      Edit Profile"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="681dp"
        android:layout_height="113dp"
        android:layout_x="-149dp"
        android:layout_y="0dp"
        app:srcCompat="@drawable/border" />

    <ImageView
        android:id="@+id/tool"
        android:layout_width="62dp"
        android:layout_height="37dp"
        android:layout_x="186dp"
        android:layout_y="25dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="169dp"
        android:layout_y="63dp"
        android:text="Settings"
        android:textColor="#ffffff"
        android:textSize="24sp"
        android:textStyle="bold|italic" />

    <TextView
        android:id="@+id/soundstatus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="205dp"
        android:layout_y="253dp"
        android:text="On"
        android:textColor="#000000"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_x="120dp"
        android:layout_y="235dp"
        android:fontFamily="casual"
        android:text="Sound"
        android:textColor="#4C4C4B"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="64dp"
        android:layout_height="37dp"
        android:layout_x="48dp"
        android:layout_y="410dp"
        android:fontFamily="casual"
        android:text="  Name"
        android:textColor="#4C4C4B"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_x="49dp"
        android:layout_y="474dp"
        android:fontFamily="casual"
        android:text="Username"
        android:textColor="#4C4C4B"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="83dp"
        android:layout_height="37dp"
        android:layout_x="46dp"
        android:layout_y="542dp"
        android:fontFamily="casual"
        android:text=" Password"
        android:textColor="#4C4C4B"
        android:textSize="14sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_x="37dp"
        android:layout_y="605dp"
        android:fontFamily="casual"
        android:text="   Birthdate"
        android:textColor="#4C4C4B"
        android:textSize="14sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/soundbutton"
        android:layout_width="28dp"
        android:layout_height="27dp"
        android:layout_x="204dp"
        android:layout_y="229dp"
        app:srcCompat="@drawable/on" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" />

    <ImageView
        android:id="@+id/logininsideprofile"
        android:layout_width="27dp"
        android:layout_height="22dp"
        android:layout_x="196dp"
        android:layout_y="311dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/emailview"
        android:layout_width="24dp"
        android:layout_height="28dp"
        android:layout_x="68dp"
        android:layout_y="446dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <EditText
        android:id="@+id/username"
        android:layout_width="224dp"
        android:layout_height="wrap_content"
        android:layout_x="167dp"
        android:layout_y="452dp"
        android:fontFamily="sans-serif-condensed"
        android:inputType="textEmailAddress" />

    <EditText
        android:id="@+id/firstname"
        android:layout_width="88dp"
        android:layout_height="wrap_content"
        android:layout_x="170dp"
        android:layout_y="385dp"
        android:fontFamily="sans-serif-condensed"
        android:hint=""
        android:inputType="text" />

    <EditText
        android:id="@+id/lastname"
        android:layout_width="94dp"
        android:layout_height="wrap_content"
        android:layout_x="299dp"
        android:layout_y="382dp"
        android:fontFamily="sans-serif-condensed"
        android:hint=""
        android:inputType="text" />

    <EditText
        android:id="@+id/password"
        android:layout_width="216dp"
        android:layout_height="wrap_content"
        android:layout_x="177dp"
        android:layout_y="498dp"
        android:fontFamily="sans-serif-condensed"
        android:hint="..........."
        android:inputType="textEmailAddress"
        android:textSize="20sp" />

    <ImageView
        android:id="@+id/passwordsettings"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_x="69dp"
        android:layout_y="512dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/birthdateview"
        android:layout_width="28dp"
        android:layout_height="40dp"
        android:layout_x="66dp"
        android:layout_y="572dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <DatePicker
        android:id="@+id/birthdate"
        android:layout_width="277dp"
        android:layout_height="103dp"
        android:layout_x="151dp"
        android:layout_y="571dp"
        android:calendarViewShown="false"
        android:datePickerMode="spinner" />

    <Button
        android:id="@+id/save"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_x="110dp"
        android:layout_y="692dp"
        android:background="@drawable/roundbutton"
        android:fontFamily="sans-serif-condensed"
        android:text="Save Changes"
        android:textAllCaps="false"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/namesettings"
        android:layout_width="34dp"
        android:layout_height="30dp"
        android:layout_x="63dp"
        android:layout_y="381dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/lastnamepencil"
        android:layout_width="24dp"
        android:layout_height="19dp"
        android:layout_x="271dp"
        android:layout_y="395dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/firstnamepencil"
        android:layout_width="24dp"
        android:layout_height="19dp"
        android:layout_x="145dp"
        android:layout_y="397dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/usernamepencil"
        android:layout_width="24dp"
        android:layout_height="19dp"
        android:layout_x="141dp"
        android:layout_y="468dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <ImageView
        android:id="@+id/passwordpencil"
        android:layout_width="24dp"
        android:layout_height="19dp"
        android:layout_x="149dp"
        android:layout_y="531dp"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="51dp"
        android:layout_y="647dp"

        android:text="Logout" />

</AbsoluteLayout>
